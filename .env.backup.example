# Cloudflare R2 settings for backup uploads
R2_ACCOUNT_ID=your_account_id
R2_BUCKET=geosolutions-prod-backups
R2_ACCESS_KEY_ID=your_r2_access_key_id
R2_SECRET_ACCESS_KEY=your_r2_secret_access_key

# Optional overrides
R2_REGION=auto
R2_PREFIX=geosolutions/prod
R2_ENDPOINT=

# Keep local files after upload (true/false)
KEEP_LOCAL_BACKUPS=false

# Optional local working directory
# BACKUP_WORK_DIR=/var/backups/geosolutions
