{
  "kind": "collectionType",
  "collectionName": "testimonials",
  "info": {
    "singularName": "testimonial",
    "pluralName": "testimonials",
    "displayName": "Testimonial",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "quote": {
      "type": "richtext",
      "required": true
    },
    "author": {
      "type": "string",
      "required": true
    },
    "company": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "order": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project"
    }
  }
}
